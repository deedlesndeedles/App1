<Page
    x:Class="App1.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:App1"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Margin="0,0,0.429,0.429">

        <ScrollViewer x:Name="Main" Margin="0" MinZoomFactor="1.4" MaxZoomFactor="1.4" ZoomMode="Disabled" Padding="0" >

            <WebView x:Name="Viewer" RenderTransformOrigin="8,8" ScrollViewer.VerticalScrollBarVisibility="Auto" IsTapEnabled="False" IsRightTapEnabled="False" IsHoldingEnabled="False" IsDoubleTapEnabled="False" Height="100" Width="100" IsHitTestVisible="False" DefaultBackgroundColor="Black" ManipulationMode="None" ScrollViewer.HorizontalScrollBarVisibility="Auto"/>
        </ScrollViewer>
        <Button x:ConnectionId='1' x:Name="ButtonNext" Content="" HorizontalAlignment="Stretch" Margin="1182,0,0,0" VerticalAlignment="Stretch" BorderBrush="{x:Null}" Foreground="#33FFFFFF" Opacity="0.1"                         />
    </Grid>
</Page>

